{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|webp|ico|woff|woff2|ttf|eot))",
        "dest": "/$1"
      },
      {
        "src": "/(.*\\.html)",
        "dest": "/$1"
      },
      {
        "src": "/",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "functions": {
      "server.js": {
        "maxDuration": 30
      }
    }
  }
  